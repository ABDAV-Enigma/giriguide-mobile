{
  "expo": {
    "name": "giri-guide",
    "permissions": [
      "CAMERA",
      "CAMERA_ROLL"
    ],
    "slug": "giri-guide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoFixedNoText.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.margarethanaibaho2.giriguide"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "assets": [
      "./assets/fonts/"
    ],
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7ed6cfb4-d7ec-4442-a640-ca6182500989"
      }
    }
  }
}
